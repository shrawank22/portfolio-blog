    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <div class="title-box text-center">
            <h3 class="title-a">
              Services
            </h3>
            <p class="subtitle-a">
              Dummy Data
            </p>
            <div class="line-mf"></div>
          </div>
        </div>
      </div>
      <div class="row">
        <% for(details of serviceDetails) { %>
          <div class="col-md-4">
            <div class="service-box">
              <div class="service-ico">
                <span class="ico-circle"><i class="<%= details.icon %>"></i></span>
              </div>
              <div class="service-content">
                <h2 class="s-title">
                  <%= details.title %>
                </h2>
                <p class="s-description text-center">
                  <%= details.desc %>
                </p>
              </div>
            </div>
          </div>
          <% } %>
      </div>
    </div>
  </section>
  <!--/ Section Services End /-->


  <!--/ Section Portfolio Star /-->
