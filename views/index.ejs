<%- include("partials/header") %>


  <!--/ Intro Skew Star /-->
  <div id="home" class="intro route bg-image"
    style="background-image: url(https://images.unsplash.com/photo-1605478328994-f93e98217da7?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1800&q=80)">
    <div class="overlay-itro"></div>
    <div class="intro-content display-table">
      <div class="table-cell">
        <div class="container">
          <h1 class="intro-title mb-4"><strong>
              <%= homeDetails.intro %>
            </strong></h1>
          <p class="intro-subtitle">
            <% for(details of homeDetails.slider) { %>
              <span class="text-slider-items">
                <%= details.text %>,
              </span>
              <%}%><strong class="text-slider"></strong>
          </p>
        </div>
      </div>
    </div>
